<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".find_photographer.FindPhotographerActivity">



    <com.google.android.material.bottomnavigation.BottomNavigationView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/white"
            style="@style/Widget.Design.BottomNavigationView"
            app:menu="@menu/photo_session_item"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/findPhotographerBottomNavigationView"/>

    <androidx.viewpager.widget.ViewPager
            android:id="@+id/findPhotographerViewPager"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/findPhotographerBottomNavigationView"/>
</androidx.constraintlayout.widget.ConstraintLayout>